{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next/babel", "next/core-web-vitals", "prettier","eslint:recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
    "prettier/prettier": "error"
  }
}
